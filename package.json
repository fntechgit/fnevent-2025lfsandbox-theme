{
  "name": "fnevent-tier1-default-theme",
  "version": "1.0.0",
  "private": true,
  "description": "Tier 1 Default fnEvent theme",
  "author": "FNTech",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop -S -H 0.0.0.0",
    "start": "gatsby develop",
    "build": "bash scripts/build-with-sharp-fix.sh",
    "serve": "gatsby serve",
    "gatsby-clean": "gatsby clean",
    "clean": "find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' + && yarn && yarn gatsby-clean",
    "postinstall": "node scripts/postinstall.js"
  },
  "dependencies": {
    "@openeventkit/event-site": "2.1.34",
    "gatsby": "^5.13.5",
    "sharp": "0.34.4"
  },
  "devDependencies": {
    "babel-preset-gatsby": "^3.14.0",
    "jsdom": "^24.0.0",
    "matchmediaquery": "^0.3.1",
    "sass-embedded": "^1.82.0",
    "xmlhttprequest": "^1.8.0"
  },
  "resolutions": {
    "**/sass-loader": "^13.3.3",
    "**/sharp": "0.34.4"
  },
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "graphql": "^16.8.1",
    "sass-loader": "^13.3.3",
    "sharp": "0.34.4"
  }
}
